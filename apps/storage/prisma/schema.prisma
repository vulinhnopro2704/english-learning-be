generator client {
  provider        = "prisma-client"
  output          = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model File {
  id        String   @id @default(uuid())
  filename  String   @db.VarChar(255)
  url       String   @db.Text
  mimeType  String   @db.VarChar(100)
  size      Int
  uploadedBy Int
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
}
